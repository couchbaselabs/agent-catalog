-- Note: all_sessions.sqlpp should be run before this script.
CREATE OR REPLACE FUNCTION
    `[BUCKET_NAME]`.`[SCOPE_NAME]`.QueryLastSession () {
        (
            FROM
                `[BUCKET_NAME]`.`[SCOPE_NAME]`.QuerySessions() AS s
            SELECT VALUE
                s.sid
            ORDER BY
                s.start_t DESC
            LIMIT 1
        )[0]
    };
